    <title type="text">Ole Martin Årst gir seg</title>
    <published>2012-11-05T18:37:06.000Z</published>
    <author>
        <name>Ola Nordmann</name>
        <uri>http://apitestbeta3.medianorge.no/news/publication/common/searchContents/instance/?id=1014&amp;contentType=person</uri>
